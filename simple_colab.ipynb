    {
      "cell_type": "code",
      "execution_count": null,
      "metadata": {},
      "outputs": [],
      "source": [
        "# Simple face swap using original Deep-Live-Cam code\n",
        "import sys\n",
        "import cv2\n",
        "import tempfile\n",
        "sys.path.append('.')\n",
        "\n",
        "# Import original modules\n",
        "import modules.globals\n",
        "from modules.processors.frame.core import get_frame_processors_modules\n",
        "from modules.utilities import is_video, create_temp, extract_frames, get_temp_frame_paths, create_video, restore_audio, clean_temp, detect_fps\n",
        "\n",
        "def simple_face_swap(source_path, target_path, output_path, mouth_mask=False, many_faces=False, map_faces=False):\n",
        "    \"\"\"Simple face swap using original Deep-Live-Cam logic\"\"\"\n",
        "    \n",
        "    # Set global variables (same as original)\n",
        "    modules.globals.headless = True\n",
        "    modules.globals.execution_providers = ['CUDAExecutionProvider', 'CPUExecutionProvider']\n",
        "    modules.globals.frame_processors = ['face_swapper']\n",
        "    modules.globals.source_path = source_path\n",
        "    modules.globals.target_path = target_path\n",
        "    modules.globals.output_path = output_path\n",
        "    modules.globals.mouth_mask = mouth_mask\n",
        "    modules.globals.many_faces = many_faces\n",
        "    modules.globals.map_faces = map_faces\n",
        "    modules.globals.keep_fps = True\n",
        "    modules.globals.keep_audio = True\n",
        "    modules.globals.video_encoder = 'libx264'\n",
        "    modules.globals.video_quality = 18\n",
        "    \n",
        "    print(f\"Processing: {source_path} -> {target_path}\")\n",
        "    print(f\"Options: mouth_mask={mouth_mask}, many_faces={many_faces}, map_faces={map_faces}\")\n",
        "    \n",
        "    # Get frame processors (original logic)\n",
        "    frame_processors = get_frame_processors_modules(modules.globals.frame_processors)\n",
        "    \n",
        "    # Pre-start checks\n",
        "    for frame_processor in frame_processors:\n",
        "        if not frame_processor.pre_start():\n",
        "            return False, \"Pre-start check failed\"\n",
        "    \n",
        "    if is_video(target_path):\n",
        "        # Video processing (original logic)\n",
        "        print(\"Processing video...\")\n",
        "        \n",
        "        if not map_faces:\n",
        "            create_temp(target_path)\n",
        "            extract_frames(target_path)\n",
        "        \n",
        "        temp_frame_paths = get_temp_frame_paths(target_path)\n",
        "        \n",
        "        # Process frames\n",
        "        for frame_processor in frame_processors:\n",
        "            print(f\"Processing with {frame_processor.NAME}...\")\n",
        "            frame_processor.process_video(source_path, temp_frame_paths)\n",
        "        \n",
        "        # Create video with FPS\n",
        "        fps = detect_fps(target_path)\n",
        "        print(f\"Creating video with {fps} fps...\")\n",
        "        create_video(target_path, fps)\n",
        "        \n",
        "        # Restore audio\n",
        "        print(\"Restoring audio...\")\n",
        "        restore_audio(target_path, output_path)\n",
        "        \n",
        "        # Cleanup\n",
        "        clean_temp(target_path)\n",
        "        \n",
        "        return True, \"Video processing completed!\"\n",
        "        \n",
        "    else:\n",
        "        # Image processing (original logic)\n",
        "        print(\"Processing image...\")\n",
        "        \n",
        "        # Copy target to output first\n",
        "        import shutil\n",
        "        shutil.copy2(target_path, output_path)\n",
        "        \n",
        "        # Process image\n",
        "        for frame_processor in frame_processors:\n",
        "            print(f\"Processing with {frame_processor.NAME}...\")\n",
        "            frame_processor.process_image(source_path, output_path, output_path)\n",
        "        \n",
        "        return True, \"Image processing completed!\"\n",
        "\n",
        "print(\"Simple face swap ready!\")"
      ]
    },
    {
      "cell_type": "code",
      "execution_count": null,
      "metadata": {},
      "outputs": [],
      "source": [
        "# Usage examples\n",
        "source_image = \"/content/source.jpg\"  # Upload your source face image\n",
        "target_media = \"/content/target.mp4\"   # Upload your target video/image\n",
        "output_file = \"/content/result.mp4\"    # Output file\n",
        "\n",
        "# Basic face swap\n",
        "success, message = simple_face_swap(source_image, target_media, output_file)\n",
